{
  "name": "@evorada/maeve-csms-client",
  "version": "0.0.0",
  "description": "TypeScript client for MaEVe CSMS API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "openapi-generator-cli generate -i ../manager/api/api-spec.yaml -g typescript-axios -o ./src/generated --additional-properties=supportsES6=true,npmName=@evorada/maeve-csms-client,withSeparateModelsAndApi=true,apiPackage=api,modelPackage=models",
    "build": "npm run generate && tsc",
    "clean": "rm -rf dist src/generated",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "maeve",
    "csms",
    "ocpp",
    "ev-charging",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:evorada/maeve-csms.git"
  },
  "publishConfig": {
    "@evorada:registry": "https://gitlab.com/api/v4/projects/65031632/packages/npm/"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
